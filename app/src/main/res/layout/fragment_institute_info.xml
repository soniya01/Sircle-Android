<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <ImageView
            android:layout_width="172dp"
            android:layout_gravity="center"
            android:layout_height="172dp"
            android:layout_marginTop="20dp"
            android:src="@drawable/emissionsloginlogo"/>

        <com.snaptech.quaderia.custom.RobotoRegularTextView
            android:textSize="18dp"
            android:textColor="#002E49"
            android:layout_marginTop="20dp"
            android:layout_gravity="center"
            android:text="Quaderia Burhani School"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.snaptech.quaderia.custom.RobotoRegularTextView
            android:textSize="18dp"
            android:textColor="#002E49"
            android:textAlignment="center"
            android:layout_marginTop="20dp"
            android:layout_gravity="center"
            android:gravity="center_horizontal"
            android:text="Itwara Road,\nBurhanpur,\nMadhya Pradesh, 450331."
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.snaptech.quaderia.custom.RobotoRegularTextView
            android:textSize="18dp"
            android:textColor="#002E49"
            android:layout_marginTop="20dp"
            android:layout_gravity="center"
            android:text="quaderiaburhani@gmail.com"
            android:autoLink="email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.snaptech.quaderia.custom.RobotoRegularTextView
            android:textSize="18dp"
            android:textColor="#002E49"
            android:layout_marginTop="20dp"
            android:layout_gravity="center"
            android:text="+917325253537"
            android:autoLink="phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>


</FrameLayout>